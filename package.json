{
  "name": "techbadges",
  "version": "2.0.0",
  "description": "Showcase your tech stack on your GitHub or resume with ease - 290+ technology icons",
  "type": "module",
  "main": "src/index.ts",
  "license": "MIT",
  "author": "santosr2",
  "contributors": [
    {
      "name": "tandpfun",
      "url": "https://github.com/tandpfun",
      "note": "Original skill-icons creator"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/santosr2/techbadges.git"
  },
  "homepage": "https://techbadges.santosr2.xyz",
  "bugs": {
    "url": "https://github.com/santosr2/techbadges/issues"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/tandpfun"
  },
  "keywords": [
    "icons",
    "skills",
    "github",
    "readme",
    "svg",
    "cloudflare-workers",
    "developer-tools"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "build": "bun run scripts/build.ts",
    "build:optimize": "bun run scripts/optimize-icons.ts && bun run scripts/build.ts",
    "deploy": "bun run build && wrangler deploy",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "validate:icons": "bun run scripts/validate-icons.ts",
    "size-report": "bun run scripts/size-report.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@cloudflare/workers-types": "^4.20251202.0",
    "@types/bun": "^1.3.3",
    "husky": "^9.1.7",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
